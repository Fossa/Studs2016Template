        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="s"
            android:id="@+id/bs"
            android:layout_row="3"
            android:layout_column="3" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="t"
            android:id="@+id/bt"
            android:layout_row="3"
            android:layout_column="4" />

        <Button
