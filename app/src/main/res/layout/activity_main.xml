<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="se.dewire.studs2016template.MainActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/textView" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true">


        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="A"
            android:id="@+id/ba"
            android:layout_row="0"
            android:layout_column="0" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="B"
            android:id="@+id/bb"
            android:layout_row="0"
            android:layout_column="1" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="C"
            android:id="@+id/bc"
            android:layout_row="0"
            android:layout_column="2" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="D"
            android:id="@+id/bd"
            android:layout_row="0"
            android:layout_column="3" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="E"
            android:id="@+id/be"
            android:layout_row="0"
            android:layout_column="4" />

        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="F"
            android:id="@+id/bf"
            android:layout_row="1"
            android:layout_column="0" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="G"
            android:id="@+id/bg"
            android:layout_row="1"
            android:layout_column="1" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="H"
            android:id="@+id/bh"
            android:layout_row="1"
            android:layout_column="2" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="I"
            android:id="@+id/bi"
            android:layout_row="1"
            android:layout_column="3" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="J"
            android:id="@+id/bj"
            android:layout_row="1"
            android:layout_column="4" />

        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="K"
            android:id="@+id/bk"
            android:layout_row="2"
            android:layout_column="0" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="L"
            android:id="@+id/bl"
            android:layout_row="2"
            android:layout_column="1" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="M"
            android:id="@+id/bm"
            android:layout_row="2"
            android:layout_column="2" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="N"
            android:id="@+id/bn"
            android:layout_row="2"
            android:layout_column="3" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="O"
            android:id="@+id/bo"
            android:layout_row="2"
            android:layout_column="4" />

        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="P"
            android:id="@+id/bp"
            android:layout_row="3"
            android:layout_column="0" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="Q"
            android:id="@+id/bq"
            android:layout_row="3"
            android:layout_column="1" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="R"
            android:id="@+id/br"
            android:layout_row="3"
            android:layout_column="2" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="S"
            android:id="@+id/bs"
            android:layout_row="3"
            android:layout_column="3" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="T"
            android:id="@+id/bt"
            android:layout_row="3"
            android:layout_column="4" />

        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="U"
            android:id="@+id/bu"
            android:layout_row="4"
            android:layout_column="0" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="V"
            android:id="@+id/bv"
            android:layout_row="4"
            android:layout_column="1" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="W"
            android:id="@+id/bw"
            android:layout_row="4"
            android:layout_column="2" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="X"
            android:id="@+id/bx"
            android:layout_row="4"
            android:layout_column="3" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="Y"
            android:id="@+id/by"
            android:layout_row="4"
            android:layout_column="4" />

        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="Z"
            android:id="@+id/bz"
            android:layout_row="5"
            android:layout_column="0" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="Å"
            android:id="@+id/bå"
            android:layout_row="5"
            android:layout_column="1" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="Ä"
            android:id="@+id/bä"
            android:layout_row="5"
            android:layout_column="2" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="Ö"
            android:id="@+id/bö"
            android:layout_row="5"
            android:layout_column="3" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text=" "
            android:id="@+id/b_"
            android:layout_row="5"
            android:layout_column="4" />

    </GridLayout>
</RelativeLayout>
