            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="w"
            android:id="@+id/bw"
            android:layout_row="4"
            android:layout_column="2" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="x"
            android:id="@+id/bx"
            android:layout_row="4"
            android:layout_column="3" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="y"
            android:id="@+id/by"
            android:layout_row="4"
            android:layout_column="4" />

        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="z"
            android:id="@+id/bz"
            android:layout_row="5"
            android:layout_column="0" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="å"
            android:id="@+id/bå"
            android:layout_row="5"
            android:layout_column="1" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="ä"
            android:id="@+id/bä"
            android:layout_row="5"
            android:layout_column="2" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="ö"
            android:id="@+id/bö"
            android:layout_row="5"
            android:layout_column="3" />
        <Button
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text=" "
            android:id="@+id/b_"
            android:layout_row="5"
            android:layout_column="4" />

    </GridLayout>
</RelativeLayout>
